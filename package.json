{
  "name": "function-tree",
  "displayName": "Functions Navigator",
  "description": "Instantly browse and navigate functions in PHP, JavaScript, TypeScript, Python, Vue, and Svelte files with a sleek Function Navigator view in Explorer.",
  "version": "1.4.1",
  "icon": "icon.png",
  "license": "MIT",
  "publisher": "CodeUnit-TL",
  "repository": {
    "type": "git",
    "url": "https://github.com/codeunitdev/function-navigator"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Visualization"
  ],
  "keywords": [
    "function",
    "navigator",
    "explorer",
    "tree",
    "code outline",
    "symbol viewer",
    "php",
    "javascript",
    "typescript",
    "python",
    "vue",
    "svelte",
    "function browser",
    "function list",
    "jump to function"
  ],
  "activationEvents": [
    "onLanguage:php",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:python",
    "onLanguage:vue",
    "onLanguage:svelte",
    "workspaceContains:**/*.{php,js,ts,jsx,tsx,py,pyw,vue,mjs,cjs,svelte}"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "functionTreeView",
          "name": "Project + Functions",
          "icon": "$(list-tree)"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "functionTreeView",
        "contents": "No PHP, JavaScript, TypeScript, Python, Vue, or Svelte functions detected yet.\n[Open Folder](command:vscode.openFolder)"
      }
    ],
    "commands": [
      {
        "command": "functionTree.navigateToFunction",
        "title": "Navigate to Function",
        "category": "Function Tree",
        "icon": "$(search)"
      },
      {
        "command": "functionTree.refresh",
        "title": "Refresh Explore",
        "category": "Function Tree",
        "icon": "$(refresh)"
      },
      {
        "command": "functionTree.debugTree",
        "title": "Function Tree: Debug Tree",
        "category": "Function Tree",
        "icon": "$(debug-alt)"
      },
      {
        "command": "functionTree.collapseAll",
        "title": "Collapse All",
        "category": "Function Tree",
        "icon": "$(collapse-all)"
      },
      {
        "command": "functionTree.newFile",
        "title": "New File...",
        "category": "Function Tree",
        "icon": "$(new-file)"
      },
      {
        "command": "functionTree.newFolder",
        "title": "New Folder...",
        "category": "Function Tree",
        "icon": "$(new-folder)"
      },
      {
        "command": "revealFileInOS",
        "title": "Reveal in File Explorer"
      },
      {
        "command": "copyFilePath",
        "title": "Copy Path"
      },
      {
        "command": "copyRelativeFilePath",
        "title": "Copy Relative Path"
      },
      {
        "command": "functionTree.renameFile",
        "title": "Rename File"
      },
      {
        "command": "functionTree.deleteFile",
        "title": "Delete File"
      },
      {
        "command": "functionTree.openInTerminal",
        "title": "Open in Integrated Terminal"
      },
      {
        "command": "functionTree.copyFilePath",
        "title": "Copy Path"
      },
      {
        "command": "functionTree.cutFile",
        "title": "Cut"
      },
      {
        "command": "functionTree.copyFile",
        "title": "Copy"
      },
      {
        "command": "functionTree.pasteFile",
        "title": "Paste"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "functionTree.refresh",
          "when": "view == functionTreeView && viewItem == folder",
          "group": "inline"
        },
        {
          "command": "functionTree.navigateToFunction",
          "when": "view == functionTreeView && viewItem == function",
          "group": "inline"
        },
        {
          "command": "functionTree.newFile",
          "when": "view == functionTreeView && viewItem == folder",
          "group": "navigation@1",
          "title": "New File..."
        },
        {
          "command": "functionTree.newFolder",
          "when": "view == functionTreeView && viewItem == folder",
          "group": "navigation@2",
          "title": "New Folder..."
        },
        {
          "command": "functionTree.openInTerminal",
          "when": "view == functionTreeView && (viewItem == folder || viewItem == file)",
          "group": "navigation@5",
          "title": "Open in Integrated Terminal"
        },
        {
          "command": "revealFileInOS",
          "when": "view == functionTreeView && (viewItem == file || viewItem == folder)",
          "group": "2_files"
        },
        {
          "command": "copyFilePath",
          "when": "view == functionTreeView && (viewItem == file || viewItem == folder)",
          "group": "3_copy@1",
          "title": "Copy Path"
        },
        {
          "command": "copyRelativeFilePath",
          "when": "view == functionTreeView && (viewItem == file || viewItem == folder)",
          "group": "3_copy@2",
          "title": "Copy Relative Path"
        },
        {
          "command": "functionTree.cutFile",
          "when": "view == functionTreeView && (viewItem == file || viewItem == folder)",
          "group": "4_mod@1",
          "title": "Cut"
        },
        {
          "command": "functionTree.copyFile",
          "when": "view == functionTreeView && (viewItem == file || viewItem == folder)",
          "group": "4_mod@2",
          "title": "Copy"
        },
        {
          "command": "functionTree.pasteFile",
          "when": "view == functionTreeView && viewItem == folder",
          "group": "4_mod@3",
          "title": "Paste"
        },
        {
          "command": "functionTree.renameFile",
          "when": "view == functionTreeView && (viewItem == file || viewItem == folder)",
          "group": "4_mod@4",
          "title": "Rename"
        },
        {
          "command": "functionTree.deleteFile",
          "when": "view == functionTreeView && (viewItem == file || viewItem == folder)",
          "group": "4_mod@5",
          "title": "Delete"
        }
      ],
      "view/title": [
        {
          "command": "functionTree.newFile",
          "when": "view == functionTreeView",
          "group": "navigation@1",
          "icon": "$(new-file)"
        },
        {
          "command": "functionTree.newFolder",
          "when": "view == functionTreeView",
          "group": "navigation@2",
          "icon": "$(new-folder)"
        },
        {
          "command": "functionTree.refresh",
          "when": "view == functionTreeView",
          "group": "navigation@3",
          "icon": "$(refresh)"
        },
        {
          "command": "functionTree.collapseAll",
          "when": "view == functionTreeView",
          "group": "navigation@4",
          "icon": "$(collapse-all)"
        }
      ]
    },
    "configuration": {
      "title": "Function Tree",
      "properties": {
        "functionTree.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable function display and badges."
        },
        "functionTree.filterRegex": {
          "type": "string",
          "default": "",
          "description": "Regular expression to filter function names (e.g., '^get.*' to show only functions starting with 'get')."
        },
        "functionTree.pythonMaxDepth": {
          "type": "number",
          "default": 1,
          "description": "Maximum nested function depth to display in Python files (0 = top-level only)."
        },
        "functionTree.debounceMs": {
          "type": "number",
          "default": 250,
          "minimum": 25,
          "maximum": 5000,
          "description": "Delay (in milliseconds) before refreshing after text changes."
        },
        "functionTree.maxCacheEntries": {
          "type": "number",
          "default": 500,
          "minimum": 10,
          "maximum": 20000,
          "description": "Maximum number of documents to cache for parsed functions."
        }
      }
    }
  },
  "scripts": {
    "bundle": "node bundle.js",
    "vscode:prepublish": "npm run bundle",
    "watch": "esbuild src/extension.ts --bundle --platform=node --external:vscode --outfile=out/extension.js --watch",
    "tsc:watch": "tsc -watch -p ./",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/vscode": "^1.85.0",
    "esbuild": "^0.25.11",
    "typescript": "^5.9.3",
    "@vscode/codicons": "^0.0.36"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.5.22",
    "svelte": "^5.41.1",
    "typescript": "^5.9.3"
  }
}